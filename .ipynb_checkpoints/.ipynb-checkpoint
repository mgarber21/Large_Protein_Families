   "cell_type": "code",
   "execution_count": 578,
   "metadata": {},
   "outputs": [],
   "source": [
    "def TCSdomains(DB):\n",
    "    df = pd.DataFrame()\n",
    "    for version in DB.version:\n",
    "        dfTCS = get_MISTDB(version).make_TCSdf()\n",
    "        dfTCS[\"version\"] = version\n",
    "        df_temp = makeSeqDF(TCS = dfTCS)\n",
    "        df = pd.concat([df_temp, df], sort = True)\n",
    "    return df\n",
    "\n",
    "DB = Pseudomonas\n",
    "\n",
    "# df = TCSdomains(DB = DB)  \n",
    "# df.to_csv(\"TCSPlaygound_PseudmonasDB.csv\", index = False)"
   ]
